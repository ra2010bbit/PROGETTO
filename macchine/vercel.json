{
    "version": 2,
    "builds": [
      {
        "src": "PROGETTO_MACCHINE/macchine",
        "use": "@vercel/python",
        "config": {"maxLambdaSize":"15mb","runtime":"python3.9"}
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "macchine/wsgi.py"
      }
    ]
  }
  